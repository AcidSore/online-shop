<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <preConditions>
        <dbms type="h2, mysql, postgresql"/>
        <runningAs username="root"/>
    </preConditions>

    <changeSet id="0" author="AcidSore">
        <tagDatabase tag="init"/>
    </changeSet>

    <include file="changelogs/dbAddTables.xml"  relativeToChangelogFile="true" />

    <changeSet id="5" author="AcidSore">
        <tagDatabase tag="insert"/>
    </changeSet>

    <include file="changelogs/dbInsertData.xml"  relativeToChangelogFile="true" />

    <changeSet id="7" author="AcidSore">
        <tagDatabase tag="insert"/>
    </changeSet>

</databaseChangeLog>

